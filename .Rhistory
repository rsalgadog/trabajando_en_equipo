tablac1[8,"SigmaB"] <- salida_Cu$coefficients[2,2]
tablac1[8,"A"] <- salida_Cu$coefficients[1,1]
tablac1[8,"SigmaA"] <- salida_Cu$coefficients[1,2]
tablac1[8,"Alpha"] <- salida_Cu$coefficients[2,1]/salida_Cu$coefficients[1,1]
tablac1[8,"SigmaAlpha"] <- (salida_Cu$coefficients[2,2])/(salida_Cu$coefficients[2,1]) +(salida_Cu$coefficients[1,2])/(salida_Cu$coefficients[1,1])
